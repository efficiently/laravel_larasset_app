{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tortuetorche/Turbolinks"
    },
    {
      "type": "vcs",
      "url": "https://github.com/efficiently/turbolinks"
    }
  ],
  "require": {
    "laravel/framework": "~5.1.8",
    "anahkiasen/former": "~4.0.0",
    "patricktalmadge/bootstrapper": "~5.7.0",
    "efficiently/larasset": "1.1.*",
    "efficiently/turbolinks": "dev-turbo3"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "codeception/codeception": "~2.1.3"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
